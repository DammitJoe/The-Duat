
public class Student<fn> extends Users {
	public boolean verified = true;
	public String major;
	public String fn = firstName;
	public String ln = lastName;
	public Student(fn ln) {
		
	}
	
	@Override
	public void sayHello() {
		System.out.println("Hi my major is " + major + ". My name is " + firstName + " " + lastName);
	}
}
